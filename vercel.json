{
    "version": 2,
    "routes": [
      {
        "src": "/places/([0-9]+)",
        "dest": "/places/:id"
      },
      {
        "src": "/places/([0-9]+)/*",
        "dest": "/places/:id"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "status": 404,
        "dest": "/404.html"
      }
    ]
  }
  